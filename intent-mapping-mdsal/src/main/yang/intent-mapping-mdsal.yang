module intent-mapping-mdsal {
    yang-version 1;
    namespace "urn:opendaylight:params:xml:ns:yang:intent:mapping:mdsal";
    prefix "intent-mapping-mdsal";

    description
        "Model definition for intent-mapping-mdsal module";

    revision "2015-11-11" {
        description "Initial revision of intent-mapping-mdsal model";
    }

    grouping map {
        list outer-map {
            key "id";
            leaf id {
              type string;
            }
            list inner-map {
                leaf inner-key {
                    type string;
                }
                leaf value {
                     type string;
                }
            }
        }
    }
    container mappings {
         uses map;
     }
}
